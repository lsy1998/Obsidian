{
  "spellcheck": true,
  "pdfExportSettings": {
    "includeName": true,
    "pageSize": "A4",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
  },
  "spellcheckLanguages": [
    "zh-CN",
    "en-US"
  ],
  "vimMode": false,
  "attachmentFolderPath": "./attachments",
  "newFileLocation": "current",
  "trashOption": "local",
  "alwaysUpdateLinks": true,
  "userIgnoreFilters": [
    "Blog/node_modules/",
    "Blog/scaffolds/",
    "Blog/themes/",
    "Blog/public/"
  ]
}